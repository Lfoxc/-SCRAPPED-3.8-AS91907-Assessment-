{
  "manifest_version": 3,
  "name": "Pop Dictionary",
  "version": "1.0",
  "content_scripts":[
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }],

  "action": {
    "default_popup": "popup.html",
    "default_title": "Pop Dictionary"
  },

  "background": {
    "service-worker": "background.js"
  }
}
